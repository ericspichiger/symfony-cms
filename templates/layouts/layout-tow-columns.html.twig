<!DOCTYPE html>
<html lang="fr">
	<head>
		{% block head %}
			{% include "_partials/head.html.twig" %}
		{% endblock %}
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
            
		{% endblock %}

		{% block javascripts %}
        <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.2.2/js/bootstrap.min.js" integrity="sha512-5BqtYqlWfJemW5+v+TZUs22uigI8tXeVah5S/1Z6qBLVO7gakAOtkOzUtgq6dsIo5c0NJdmGPs0H9I+2OHUHVQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.2.2/js/bootstrap.bundle.min.js" integrity="sha512-BOsvKbLb0dB1IVplOL9ptU1EYA+LuCKEluZWRUYG73hxqNBU85JBIBhPGwhQl7O633KtkjMv8lvxZcWP+N3V3w==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        {{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body id="" class="">

		<header id="header">
			{% block header %}
				{% include "_partials/header.html.twig" %}
			{% endblock %}
		</header>

		<main id="content" class="content">
			<div class="container">
				<div class="row">
					{% block content_left_container %}
						<div class="col col-left col-xs-12 col-sm-4 col-md-3">
							{% block content_left %}{% endblock %}
						</div>
					{% endblock %}

					{% block content_container %}
						<div class="col col-center col-xs-12 col-sm-4 col-md-3">
							{% block content %}{% endblock %}
						</div>
					{% endblock %}

					{% block content_right_container %}
						<div class="col col-right col-xs-12 col-sm-4 col-md-3">
							{% block content_right %}{% endblock %}
						</div>
					{% endblock %}
				</div>
			</div>
		</main>

		<footer id="footer" class="footer">
			{% block footer %}
				{% include "_partials/footer.html.twig" %}
			{% endblock %}
		</footer>
	</body>
</html>
